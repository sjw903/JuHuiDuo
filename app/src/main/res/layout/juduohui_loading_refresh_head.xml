<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/txt"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/juduohui_loading_shape"
        android:layout_centerInParent="true"
        />

    <com.android.jdhshop.common.WaveLoadingView
        android:id="@+id/waveLoadingView22"
        android:visibility="invisible"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_centerInParent="true"
        />
    <ImageView
        android:id="@+id/logo_loading"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@mipmap/waveloading"
        app:layout_constraintBottom_toBottomOf="@+id/txt"
        app:layout_constraintEnd_toEndOf="@+id/txt"
        app:layout_constraintStart_toStartOf="@+id/txt"
        app:layout_constraintTop_toTopOf="@+id/txt"
        android:layout_centerInParent="true"
        />
</RelativeLayout>