<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="@dimen/margin_size88"
    android:gravity="center"
    android:orientation="vertical">

    <me.panpf.sketch.SketchImageView
        android:id="@+id/service_icon1"
        android:layout_width="@dimen/margin_size45"
        android:layout_height="@dimen/margin_size45"
         />

    <TextView
        android:id="@+id/service_name"
        style="@style/textContent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:maxLines="1"
        android:ellipsize="end"
        android:layout_marginTop="@dimen/margin_size10"
        tools:text="使用教程" />

</LinearLayout>
