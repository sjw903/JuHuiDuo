    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" /> <!-- 显示系统窗口 -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" /> <!-- 读外部存储的权限 -->
    <uses-permission android:name="android.permission.EXPAND_STATUS_BAR" /> <!-- 允许程序扩展或收缩状态栏 -->
    <uses-permission android:name="android.permission.QUERY_ALL_PACKAGES" /> <!-- 获取设备中的所有的应用 -->
